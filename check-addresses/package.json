{
	"name": "@mailchain/example-check-addresses",
	"version": "0.0.6",
	"description": "Use Mailchain SDK to check addresses to see if they are registered with Mailchain",
	"license": "MIT",
	"dependencies": {
		"@mailchain/sdk": "0.18.3",
		"axios": "1.3.4"
	},
	"scripts": {
		"start": "npm run build && node dist/index.mjs",
		"dev": "ts-node --transpile-only src/index.ts",
		"build": "rollup -c",
		"build:tsc": "tsc && tsc-alias",
		"test": "jest --passWithNoTests --forceExit --detectOpenHandles",
		"lint": "eslint --ignore-path .gitignore --ext .ts src/",
		"lint:fix": "npm run lint -- --fix"
	},
	"devDependencies": {
		"@rollup/plugin-terser": "^0.4.0",
		"@rollup/plugin-typescript": "^11.0.0",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.220",
		"@types/jest": "^28.1.6",
		"@types/node": "^18.11.18",
		"jest": "^28.1.1",
		"rollup": "^3.18.0",
		"ts-jest": "^28.0.7",
		"ts-node": "^10.9.1",
		"tsc-alias": "^1.7.0",
		"tsconfig-paths": "^4.0.0",
		"tslib": "^2.5.0",
		"typescript": "^4.9.4"
	}
}
